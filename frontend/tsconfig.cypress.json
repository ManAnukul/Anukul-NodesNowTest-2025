{
  "compilerOptions": {
    "composite": true,
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "types": ["cypress", "node", "mocha"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.tsx",
    "node_modules/cypress/types/index.d.ts"
  ]
}
